@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<!DOCTYPE html>
<!-- upto 2 directory depth-->
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Super Folio</title>
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="crossorigin" />
    <link rel="preload" as="style" href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@300;400;700;800&amp;display=swap" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@300;400;700;800&amp;display=swap" media="print" onload="this.media='all'" />
    <noscript>
        <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@300;400;700;800&amp;display=swap" />
    </noscript>
    <link href="~/resume/css/font-awesome/css/all.min.css?ver=1.2.0" rel="stylesheet">
    <link href="~/resume/css/bootstrap-icons/bootstrap-icons.css?ver=1.2.0" rel="stylesheet">
    <link href="~/resume/css/bootstrap.min.css?ver=1.2.0" rel="stylesheet">
    <link href="~/resume/css/aos.css?ver=1.2.0" rel="stylesheet">
    <link href="~/resume/css/main.css?ver=1.2.0" rel="stylesheet">
    <noscript>
        <style type="text/css">
            [data-aos] {
                opacity: 1 !important;
                transform: translate(0) scale(1) !important;
            }
        </style>
    </noscript>
</head>
<body id="top">

    <div class="page-content">
        <div id="content">

            @await Component.InvokeAsync("BusinessCard")

            @await Component.InvokeAsync("Interest")

            @await Component.InvokeAsync("Skills")
            @await Component.InvokeAsync("Portfolio")
            @await Component.InvokeAsync("Experience")

            <br />
            <br />
            <br />
            <br />
        </div>
    </div>
    <div id="scrolltop"><a class="btn btn-secondary" href="#top"><span class="icon"><i class="fas fa-angle-up fa-x"></i></span></a></div>
    <script src="~/resume/scripts/imagesloaded.pkgd.min.js?ver=1.2.0"></script>
    <script src="~/resume/scripts/masonry.pkgd.min.js?ver=1.2.0"></script>
    <script src="~/resume/scripts/BigPicture.min.js?ver=1.2.0"></script>
    <script src="~/resume/scripts/purecounter.min.js?ver=1.2.0"></script>
    <script src="~/resume/scripts/bootstrap.bundle.min.js?ver=1.2.0"></script>
    <script src="~/resume/scripts/aos.min.js?ver=1.2.0"></script>
    <script src="~/resume/scripts/main.js?ver=1.2.0"></script>
</body>
</html>